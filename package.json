  "keywords": [
    "seneca",
    "plugin",
    "repl"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/senecajs/seneca-repl.git"
  },
  "dependencies": {
    "jsonic": "0.2.2",
    "lodash": "4.14.2"
  },
  "devDependencies": {
    "code": "3.0.x",
    "eslint-config-seneca": "3.x.x",
    "eslint-plugin-standard": "2.x.x",
    "lab": "11.0.x",
    "seneca": "plugin"
  },
  "files": [
    "LICENSE.txt",
    "README.md",
    "repl.js"
  ],
  "pre-commit": [
    "test"
  ]
