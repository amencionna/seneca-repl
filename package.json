{
  "name": "seneca-repl",
  "description": "Seneca plugin: seneca-repl",
  "version": "0.2.0",
  "main": "repl.js",
  "license": "MIT",
  "scripts": {
    "test": "lab -v -P test -L -t 80",
    "coveralls": "lab -s -P test -r lcov | coveralls",
