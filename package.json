    "code": "4.1.x",
    "coveralls": "2.13.x",
    "docco": "0.7.x",
    "lab": "13.1.x",
    "pre-commit": "1.1.x",
    "prettier": "1.4.x",
    "seneca": "plugin",
    "seneca-basic": "0.5.x"
  },
  "files": [
    "LICENSE",
